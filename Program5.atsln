// 5. Write a program to find the factorial of a number using function

LDI R16, HIGH(RAMEND)
OUT SPH, R16
LDI R16, LOW(RAMEND)
OUT SPL, R16

LDI R16, 2
CALL FACT

LDI R16, $FF
OUT DDRA, R16
OUT DDRB, R16
OUT PORTA, R17
OUT PORTB, R18


LAST: RJMP LAST

FACT: LDI R17, 1
LDI R19, 1
LOOP: MUL R17, R19
MOV R17, R0
MOV R18, R1
INC R19
CP R16, R19
BRCC LOOP
RET
